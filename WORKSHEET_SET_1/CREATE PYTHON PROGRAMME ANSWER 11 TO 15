    "# answer 11 create factorial programme\n",
    "\n",
    "n= int(input('Enter your Factoril Number:- '))\n",
    "factorial = 1\n",
    "i = 1\n",
    "while i<=n:\n",
    "    factorial *= i\n",
    "    i += 1\n",
    "print('Factorial of Number=  ',factorial)"
